package com.hotelmgmt.domain.test;

import java.util.Scanner;

import com.hotelmgmt.domain.Customer;
import com.hotelmgmt.service.CustomerService;

public class TestCustomerArray {

	public static void main(String[] args) 
	{
		
		CustomerService service = new CustomerService();
		
		Customer[] customerList;
		try {
			customerList = service.getAllCustomerDetails();
		} catch (Exception e) {
			
			e.printStackTrace();
		}
		
		System.out.println("*********************");
		for (int j = 0; j<customerList.length; j++)
		{
		
			System.out.println(customerList [j].getfName()+"  ");
			System.out.print(customerList [j].getlName());
		}
		
		System.out.println("*********************");
		
		
	
	}
		

	

}
