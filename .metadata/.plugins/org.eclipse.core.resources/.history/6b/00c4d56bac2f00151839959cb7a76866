package com.isko.app.util.threads;

public class CalcExImpl implements Runnable 
{
	private int a;
	private int b;

	public CalcExImpl(int x, int y)
	{
		a = x;
		b=y;
	}

	public void run() 
	{
		sub();
		
	}

	public void sub() throws InterruptedException 
	{
		
		Thread.sleep(10000);
		System.out.println("SUB" + (a - b));
	}

}
