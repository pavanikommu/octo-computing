package com.hotelmgmt.domain;

public class DemoStringService {

	public static void main(String[] args) 
	{
		String str1 = "     Hello World     ";
		
		System.out.println("str1 intitial value "+str1);
		
		System.out.println("str1 substring "+str1.substring(4));
		
		System.out.println("str1 value after substring"+str1);
	
		
		System.out.println("length "+ str1.length());
		
		System.out.println("trim version "+ str1.trim());
		
		System.out.println("trim version length"+ str1.trim().length());
		
		
	
	}

}
