package com.milan.processing;

import com.milan.Student;

public class RunnableEx extends Student implements Runnable  
{

	public RunnableEx(String name2, int height2, int weight2, char gender2) {
		super(name2, height2, weight2, gender2);
		// TODO Auto-generated constructor stub
	}


	public void run()
	{
		System.out.println("THREAD STATRTED");
		try {
			Thread.sleep(10000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		System.out.println("THREAD ENDED");
	}
	
	
	public static void main(String s[])
	{
		System.out.println("MAIN METHOD STARTED");
		ThreadEx thread1 = new ThreadEx();
		thread1.start();
		
		ThreadEx thread2 = new ThreadEx();
		thread2.start();
		
		System.out.println("MAIN METHOD ENDED");
	}
	

}
