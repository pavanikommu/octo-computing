package com.isko.app.util.threads;

public class CalcExThread extends Thread 
{
	private int a;
	private int b;

	
	public CalcExThread(int x, int y ) 
	{
		
		
	}

	
	public void run()
	{
		sum();
	}
	
	
	public void sum() 
	{
		Thread.sleep(1000000);
		System.out.println("SUM" + (a + b));
	}




}
